{
    "name": "core",
    "sourceRoot": "packages/libs/core/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": [
                "{workspaceRoot}/dist/libs/core"
            ],
            "options": {
                "command": "stencil build --config stencil.config.ts",
                "cwd": "packages/libs/core"
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "stencil build --dev --watch --serve --config stencil.config.ts",
                "cwd": "packages/libs/core"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "stencil test --spec --e2e --config stencil.config.ts",
                "cwd": "packages/libs/core"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "stencil lint --config stencil.config.ts",
                "cwd": "packages/libs/core"
            }
        }
    }
}